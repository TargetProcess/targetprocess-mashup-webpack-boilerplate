{
  "name": "targetprocess-mashup-webpack-boilerplate",
  "version": "1.0.0",
  "description": "Webpack boilerplate to create Targetprocess mashup.",
  "keywords": [
    "webpack",
    "targetprocess",
    "mashup"
  ],
  "scripts": {
    "start": "outputPath=$TARGETPROCESS_MASHUPS_PATH/$npm_package_name; rm -rf $outputPath; NODE_ENV=development webpack --config webpack/development.config.js --watch --output-path $outputPath",
    "build:library": "outputPath=build/library; rm -rf $outputPath; NODE_ENV=production webpack  --config webpack/library.config.js --output-path $outputPath --progress",
    "build:manager": "outputPath=build/manager; rm -rf $outputPath; NODE_ENV=production webpack --config webpack/manager.config.js --output-path $outputPath --progress",
    "build:all": "npm run build:library && npm run build:manager"
  },
  "devDependencies": {
    "babel-loader": "^6.2.2",
    "babel-preset-es2015": "^6.3.13",
    "combine-assets-plugin": "targetprocess/combine-assets-plugin",
    "css-loader": "^0.14.5",
    "file-loader": "^0.8.4",
    "style-loader": "^0.12.3",
    "targetprocess-mashup-config-loader": "targetprocess/targetprocess-mashup-config-loader",
    "targetprocess-mashup-manifest-loader": "targetprocess/targetprocess-mashup-manifest-loader",
    "targetprocess-mashup-webpack-plugin": "targetprocess/targetprocess-mashup-webpack-plugin",
    "webpack": "^1.9.10"
  },
  "dependencies": {},
  "author": "Targetprocess <dev@targetprocess.com>",
  "license": "MIT",
  "contributors": [
    "Aliaksei Shytkin <shitkin@targetprocess.com>"
  ]
}
